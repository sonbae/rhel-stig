GRUB_TIMEOUT=5
GRUB_DISTRIBUTOR="$(sed 's, release .*$,,g' /etc/system-release)"
GRUB_DEFAULT=saved
GRUB_DISABLE_SUBMENU=true
GRUB_TERMINAL_OUTPUT="console"
GRUB_CMDLINE_LINUX="resume=/dev/mapper/rhel-swap rd.lvm.lv=rhel/root rd.lvm.lv=rhel/swap rhgb quiet fips=1 boot=UUID=f3f77ba4-ad96-4c72-956d-5ca8bc454c54 audit=1 audit_backlog_limit=8192 pti=on vsyscall=none page_poison=1 slub_debug=P"
GRUB_DISABLE_RECOVERY="true"
GRUB_ENABLE_BLSCFG=true